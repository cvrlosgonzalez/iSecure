<p id="notice"><%= notice %></p>

<h1>Listing Cams</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Alert</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cams.each do |cam| %>
      <tr>
        <td><%= cam.title %></td>
        <td><%= cam.alert %></td>
        <td><%= link_to 'Show', cam %></td>
        <td><%= link_to 'Edit', edit_cam_path(cam) %></td>
        <td><%= link_to 'Destroy', cam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cam', new_cam_path %>
